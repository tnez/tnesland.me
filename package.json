{
  "name": "tnez--tnesland.me",
  "description": "Personal Website for Travis Nesland",
  "version": "4.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "netlify dev",
    "dev:cy": "start-test dev :8888 cy:open",
    "build": "sapper build --legacy",
    "check:format": "prettier --plugin-search-dir=. --check ./**/*.{css,html,js,json,svelte}",
    "export": "sapper export --legacy",
    "format": "prettier --plugin-search-dir=. --write ./**/*.{css,html,js,json,svelte}",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test:cy": "npm run export && start-test 'netlify dev -d __sapper__/build' :8888 cy:run"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "polka": "next",
    "sirv": "^0.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@fullhuman/postcss-purgecss": "^2.1.2",
    "@rollup/plugin-commonjs": "^11.0.0",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "autoprefixer": "^9.7.6",
    "cypress": "^4.3.0",
    "netlify-cli": "^2.46.0",
    "prettier": "1.19.1",
    "prettier-plugin-svelte": "^0.7.0",
    "rollup": "^1.20.0",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-postcss": "^2.6.4",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^4.0.4",
    "sapper": "^0.27.0",
    "start-server-and-test": "^1.11.0",
    "svelte": "^3.0.0",
    "tailwindcss": "^1.2.0"
  }
}
